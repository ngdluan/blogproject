{
  "name": "vmo-blog",
  "version": "0.0.1",
  "description": "",
  "main": "lib/index.js",
  "devDependencies": {
    "husky": "^8.0.2",
    "lint": "^0.7.0",
    "lint-staged": "^13.0.3",
    "tslint": "^6.1.3",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "lint": "tslint .",
    "format": "tslint --fix .",
    "postinstall": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "npx --no-install commitlint --edit $1"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "yarn format",
      "git add ."
    ]
  },
  "private": true
}